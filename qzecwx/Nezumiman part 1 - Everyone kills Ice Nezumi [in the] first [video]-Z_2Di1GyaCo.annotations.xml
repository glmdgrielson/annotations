  <TEXT>Eww, waiting! 
I think I&#39;ll pass.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="13.95800" y="36.11100" w="21.25000" h="11.66700" t="0:02:59.0"/>
      <rectRegion x="13.95800" y="36.11100" w="21.25000" h="11.66700" t="0:03:03.0"/>
