  <TEXT>=D</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="68.75000" y="90.55600" w="6.04200" h="7.22200" t="0:06:10.8"/>
      <rectRegion x="68.75000" y="90.55600" w="6.04200" h="7.22200" t="0:06:11.9"/>
