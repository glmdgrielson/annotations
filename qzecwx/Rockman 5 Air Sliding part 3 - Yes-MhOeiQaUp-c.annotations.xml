    <movingRegion type="rect">
      <rectRegion x="10.00000" y="0.00000" w="89.37500" h="11.66700" t="0:06:33.8"/>
      <rectRegion x="10.00000" y="0.00000" w="89.37500" h="11.66700" t="0:06:40.8"/>
