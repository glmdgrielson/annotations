    <padding top="1.98588502407" left="1.48941373825" bottom="1.98588502407" right="1.98588502407"/>
  </appearance>
  
</annotation>
<annotation id="annotation_728391" type="text" style="popup" log_data="xble=1&amp;a-v=EPj0DtAWMS0&amp;a-type=1&amp;a-id=annotation_728391&amp;ei=7NEeXP6MFYSDgwO60YSgDQ" itct="">
  <TEXT>&quot;Oooh boy, here comes a Metroid sequence~&quot;</TEXT>
