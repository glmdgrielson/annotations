    <movingRegion type="rect">
      <rectRegion x="7.38100" y="1.58700" w="86.66700" h="20.93700" t="5:37.100"/>
      <rectRegion x="7.38100" y="1.58700" w="86.66700" h="20.93700" t="5:50.100"/>
