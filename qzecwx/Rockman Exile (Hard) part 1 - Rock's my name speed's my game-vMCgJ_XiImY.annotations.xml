<annotation id="annotation_36464" type="text" style="popup" log_data="ei=GekeXNKUHYKBhgbMtorwBg&amp;a-id=annotation_36464&amp;a-type=1&amp;a-v=vMCgJ_XiImY&amp;xble=1" itct="CAkQs-UBGP___________wEiEwiShcTC6rTfAhWCgMEKHUybAm4o-B0yAml2SObEiK__hKjgvAE">
