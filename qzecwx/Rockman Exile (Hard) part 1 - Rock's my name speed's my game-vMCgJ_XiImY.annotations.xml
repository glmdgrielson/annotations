    <movingRegion type="rect">
      <rectRegion x="16.66700" y="2.77800" w="58.95800" h="11.66700" t="0:03:21.3"/>
      <rectRegion x="16.66700" y="2.77800" w="58.95800" h="11.66700" t="0:03:25.3"/>
