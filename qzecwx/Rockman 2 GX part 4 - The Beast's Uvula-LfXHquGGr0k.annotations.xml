  <TEXT>So, naturally, I cheat.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="5.83300" y="79.16700" w="27.70800" h="8.05600" t="0:01:45.1"/>
      <rectRegion x="5.83300" y="79.16700" w="27.70800" h="8.05600" t="0:01:48.1"/>
