    <movingRegion type="rect">
      <rectRegion x="56.87500" y="4.44400" w="38.95800" h="11.66700" t="0:01:8.31"/>
      <rectRegion x="56.87500" y="4.44400" w="38.95800" h="11.66700" t="0:01:12.30"/>
