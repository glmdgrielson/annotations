    <movingRegion type="rect">
      <rectRegion x="11.25000" y="4.44400" w="49.58300" h="16.11100" t="0:00:50.70"/>
      <rectRegion x="11.25000" y="4.44400" w="49.58300" h="16.11100" t="0:00:56.70"/>
