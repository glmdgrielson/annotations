    <movingRegion type="rect">
      <rectRegion x="11.04200" y="2.22200" w="82.08300" h="16.09700" t="0:23.323"/>
      <rectRegion x="11.04200" y="2.22200" w="82.08300" h="16.09700" t="0:32.200"/>
