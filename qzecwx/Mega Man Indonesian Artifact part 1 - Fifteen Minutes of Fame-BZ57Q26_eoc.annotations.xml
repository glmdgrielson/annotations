</annotation>
<annotation id="annotation_259397" type="text" style="popup" log_data="a-type=1&amp;a-id=annotation_259397&amp;a-v=BZ57Q26_eoc&amp;ei=gtEeXK6nN6qOhwa6rZyIBg&amp;xble=1" itct="CAQQs-UBGP___________wEiEwiu8I6D1LTfAhUqx8EKHboWB2Eo-B0yAml2SIf1_fW26J7PBQ">
