    <movingRegion type="rect">
      <rectRegion x="10.88500" y="0.61100" w="79.79200" h="22.25000" t="5:01.595"/>
      <rectRegion x="10.88500" y="0.61100" w="79.79200" h="22.25000" t="5:11.800"/>
