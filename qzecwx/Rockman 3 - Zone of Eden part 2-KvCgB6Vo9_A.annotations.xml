    <movingRegion type="rect">
      <rectRegion x="14.79200" y="0.00000" w="85.40600" h="18.48600" t="7:10.654"/>
      <rectRegion x="14.79200" y="0.00000" w="85.40600" h="18.48600" t="7:14.400"/>
