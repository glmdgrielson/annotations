    <movingRegion type="rect">
      <rectRegion x="14.79200" y="3.33300" w="71.87500" h="12.22200" t="0:07:01.1"/>
      <rectRegion x="14.79200" y="3.33300" w="71.87500" h="12.22200" t="0:07:08.1"/>
