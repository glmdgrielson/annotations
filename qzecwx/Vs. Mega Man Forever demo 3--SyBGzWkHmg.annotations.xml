<annotation id="annotation_516340" type="text" style="popup" log_data="ei=9dMeXKnFB7Swhwasn47QCQ&amp;xble=1&amp;a-type=1&amp;a-id=annotation_516340&amp;a-v=-SyBGzWkHmg" itct="">
