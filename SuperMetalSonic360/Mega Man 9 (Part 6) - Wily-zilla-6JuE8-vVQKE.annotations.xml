  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_2402739133" type="text" style="popup" log_data="a-v=6JuE8-vVQKE&amp;a-id=annotation_2402739133&amp;a-type=1&amp;xble=1&amp;ei=2jYgXODUBYfuhgbkrYzADw" itct="CAoQs-UBGP___________wEiEwjgqeXnqLffAhUHt8EKHeQWA_go-B0yAml2SKGB1d6-nuHN6AE">
  <TEXT>Huh... is it just me, or is that Wily machine incredibly familiar? I can&#39;t quite put my finger on it...</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.63400" y="0.00000" w="29.36600" h="19.59200" t="2:41.577"/>
      <rectRegion x="70.63400" y="0.00000" w="29.36600" h="19.59200" t="2:47.100"/>
