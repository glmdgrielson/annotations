  <TEXT>Somebody order watermelon for dessert? </TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="16.38900" t="0:10:42.1"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="16.38900" t="0:10:47.1"/>
