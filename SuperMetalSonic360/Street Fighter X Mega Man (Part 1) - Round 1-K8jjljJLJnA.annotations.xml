<annotation id="annotation_776045" type="text" style="popup" log_data="a-type=1&amp;a-v=K8jjljJLJnA&amp;xble=1&amp;ei=D0kgXN2ZNJnfhgbTpqS4CA&amp;a-id=annotation_776045" itct="">
  <TEXT>Rose... ugh. This boss is irritating. </TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="12.22200" t="0:07:22.0"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="12.22200" t="0:07:27.0"/>
