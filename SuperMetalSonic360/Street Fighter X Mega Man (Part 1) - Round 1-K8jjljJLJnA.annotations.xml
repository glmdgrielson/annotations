</annotation>
<annotation id="annotation_857302" type="text" style="popup" log_data="a-type=1&amp;a-v=K8jjljJLJnA&amp;xble=1&amp;ei=D0kgXN2ZNJnfhgbTpqS4CA&amp;a-id=annotation_857302" itct="">
  <TEXT>I haven&#39;t showed off the watermelon or Soul Satellite yet, so here we go!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.55600" t="0:09:13.7"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.55600" t="0:09:18.7"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.25" bgColor="0" fgColor="16777215" textSize="3.30980833333333" effects="textdropshadow">
