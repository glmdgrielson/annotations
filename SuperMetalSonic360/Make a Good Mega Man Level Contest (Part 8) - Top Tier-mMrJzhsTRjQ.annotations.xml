    <movingRegion type="rect">
      <rectRegion x="70.15800" y="0.00000" w="29.84200" h="27.47900" t="9:36.846"/>
      <rectRegion x="70.15800" y="0.00000" w="29.84200" h="27.47900" t="9:43.300"/>
