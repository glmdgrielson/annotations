    <movingRegion type="rect">
      <rectRegion x="69.04900" y="0.00000" w="30.95100" h="26.07000" t="0:21.114"/>
      <rectRegion x="69.04900" y="0.00000" w="30.95100" h="26.07000" t="0:27.600"/>
