    <movingRegion type="rect">
      <rectRegion x="70.63400" y="0.00000" w="29.36600" h="19.87300" t="6:11.366"/>
      <rectRegion x="70.63400" y="0.00000" w="29.36600" h="19.87300" t="6:16.900"/>
