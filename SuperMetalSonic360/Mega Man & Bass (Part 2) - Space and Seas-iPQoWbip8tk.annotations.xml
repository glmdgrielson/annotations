    <movingRegion type="rect">
      <rectRegion x="69.68300" y="0.00000" w="30.31700" h="24.66200" t="9:52.029"/>
      <rectRegion x="69.68300" y="0.00000" w="30.31700" h="24.66200" t="9:58.500"/>
