    <movingRegion type="rect">
      <rectRegion x="70.47500" y="0.00000" w="29.52500" h="18.46500" t="8:07.646"/>
      <rectRegion x="70.47500" y="0.00000" w="29.52500" h="18.46500" t="8:14.100"/>
