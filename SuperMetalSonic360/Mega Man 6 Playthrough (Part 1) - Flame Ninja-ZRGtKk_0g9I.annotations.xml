  <TEXT>D</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="71.45800" y="20.27800" w="6.04200" h="8.88900" t="0:03:11.0"/>
      <rectRegion x="71.45800" y="20.27800" w="6.04200" h="8.88900" t="0:03:21.0"/>
