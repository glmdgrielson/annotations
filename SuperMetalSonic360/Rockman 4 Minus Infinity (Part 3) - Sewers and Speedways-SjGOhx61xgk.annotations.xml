  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_2012945347" type="text" style="popup" log_data="a-v=SjGOhx61xgk&amp;a-id=annotation_2012945347&amp;a-type=1&amp;xble=1&amp;ei=uj4gXLjHBdqU8wSF9pC4DQ" itct="CAgQs-UBGP___________wEiEwi4jIypsLffAhVaypwKHQU7BNco-B0yAml2SImM1_Xx0OOYSg">
  <TEXT>The name of this mini boss is too long to say here (it&#39;s in the description if you&#39;re curious). They are very easy to deal with though.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.36600" y="0.00000" w="30.63400" h="19.59200" t="0:59.753"/>
      <rectRegion x="69.36600" y="0.00000" w="30.63400" h="19.59200" t="1:04.800"/>
