    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="30.41700" h="49.72200" t="0:03:33.7"/>
      <rectRegion x="0.00000" y="0.00000" w="30.41700" h="49.72200" t="0:03:41.7"/>
