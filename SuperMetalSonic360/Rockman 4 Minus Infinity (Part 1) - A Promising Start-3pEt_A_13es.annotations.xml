    <movingRegion type="rect">
      <rectRegion x="69.84200" y="0.00000" w="30.15800" h="19.87300" t="1:15.515"/>
      <rectRegion x="69.84200" y="0.00000" w="30.15800" h="19.87300" t="1:20.500"/>
