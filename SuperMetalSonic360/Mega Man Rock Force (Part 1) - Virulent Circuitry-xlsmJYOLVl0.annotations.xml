<annotation id="annotation_1912181039" type="text" style="popup" log_data="xble=1&amp;a-type=1&amp;a-v=xlsmJYOLVl0&amp;ei=RD8gXJv6OJbYhgbX0YnAAQ&amp;a-id=annotation_1912181039" itct="CAcQs-UBGP___________wEiEwibrKbrsLffAhUWrMEKHddoAhgo-B0yAml2SN2srZzYxMmtxgE">
  <TEXT>This is probably one of my favourite weapon gets ever. It does all the explaining for me!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="15.27800" t="8:16.524"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="15.27800" t="8:21.500"/>
