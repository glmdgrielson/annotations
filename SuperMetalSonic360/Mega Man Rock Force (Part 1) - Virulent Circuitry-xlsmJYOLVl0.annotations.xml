    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="22.77800" t="1:53.878"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="22.77800" t="1:58.900"/>
