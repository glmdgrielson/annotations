    <movingRegion type="rect">
      <rectRegion x="42.47200" y="0.00000" w="30.15800" h="28.04200" t="5:00.965"/>
      <rectRegion x="42.47200" y="0.00000" w="30.15800" h="28.04200" t="5:07.500"/>
