    <movingRegion type="rect">
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="24.66200" t="2:40.095"/>
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="24.66200" t="2:45.600"/>
