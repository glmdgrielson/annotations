    <movingRegion type="rect">
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="20.71800" t="9:34.945"/>
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="20.71800" t="9:40.400"/>
