    <movingRegion type="rect">
      <rectRegion x="69.36600" y="0.00000" w="30.63400" h="27.19700" t="1:08.203"/>
      <rectRegion x="69.36600" y="0.00000" w="30.63400" h="27.19700" t="1:14.700"/>
