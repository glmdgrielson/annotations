    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="24.72200" t="0:00:45.7"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="24.72200" t="0:00:50.7"/>
