<annotation id="annotation_205074" type="text" style="popup" log_data="xble=1&amp;ei=HUUgXKiyBsavhwb4ia3ACQ&amp;a-v=P2dXen-pgo8&amp;a-id=annotation_205074&amp;a-type=1" itct="CAQQs-UBGP___________wEiEwjotN20trffAhXG18EKHfhEC5go-B0yAml2SI-Fpv2n79WzPw">
  <TEXT>This room is filled with lighting traps on the floor and bullets and enemies flying everywhere. Not a bad place for the leaf shield.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="28.88900" t="0:01:37.8"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="28.88900" t="0:01:42.8"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.30980833333333" effects="textdropshadow">
