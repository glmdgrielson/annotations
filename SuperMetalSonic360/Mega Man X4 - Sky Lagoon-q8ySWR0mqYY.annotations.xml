<annotation id="annotation_2022732629" type="text" style="popup" log_data="a-v=q8ySWR0mqYY&amp;a-id=annotation_2022732629&amp;ei=3K8fXOz7CaOp8gTuxoigAg&amp;xble=1&amp;a-type=1" itct="CAUQs-UBGP___________wEiEwjs2aSJqLbfAhWjlJwKHW4jAiQo-B0yAml2SIbTmumRy6TmqwE">
  <TEXT>One important control change from X3 to X4 is that holding the dash button turns your next jump into a dash jump. It means jumping is usually the most efficient way to travel.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.36600" y="0.00000" w="30.63400" h="29.73200" t="4:04.322"/>
      <rectRegion x="69.36600" y="0.00000" w="30.63400" h="29.73200" t="4:10.700"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
