  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_2883848271" type="text" style="popup" log_data="xble=1&amp;a-type=1&amp;a-v=uaGqHASTUEY&amp;ei=oTcgXIDYBIyahwbYvLLoBw&amp;a-id=annotation_2883848271" itct="">
  <TEXT>This flower clock is called Hanabiren. You want to avoid the hand turning around, but remain in a good position to attack the flower.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.15800" y="0.00000" w="29.84200" h="25.22500" t="8:38.910"/>
      <rectRegion x="70.15800" y="0.00000" w="29.84200" h="25.22500" t="8:44.400"/>
