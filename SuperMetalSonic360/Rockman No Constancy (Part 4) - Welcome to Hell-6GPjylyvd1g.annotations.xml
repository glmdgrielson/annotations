    <movingRegion type="rect">
      <rectRegion x="70.37000" y="0.00000" w="29.63000" h="28.05600" t="3:20.762"/>
      <rectRegion x="70.37000" y="0.00000" w="29.63000" h="28.05600" t="3:25.800"/>
