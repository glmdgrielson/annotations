    <movingRegion type="rect">
      <rectRegion x="70.37000" y="0.00000" w="29.63000" h="23.61100" t="6:06.967"/>
      <rectRegion x="70.37000" y="0.00000" w="29.63000" h="23.61100" t="6:12.000"/>
