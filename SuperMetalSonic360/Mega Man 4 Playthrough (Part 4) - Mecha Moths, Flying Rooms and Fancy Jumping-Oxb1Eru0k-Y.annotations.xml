<?xml version="1.0" encoding="UTF-8" ?><document><annotations itct="CAAQwOYBIhMIi-TIsry33wIVzerBCh3c7A3aKPgdMgJpdkjmp9Ldq6K9izs">
<annotation id="annotation_11316" type="text" style="popup" log_data="a-type=1&amp;a-id=annotation_11316&amp;xble=1&amp;ei=Y0sgXMumC83Vhwbc2bfQDQ&amp;a-v=Oxb1Eru0k-Y" itct="CAEQs-UBGP___________wEiEwiL5MiyvLffAhXN6sEKHdzsDdoo-B0yAml2SOan0t2ror2LOw">
