    <movingRegion type="rect">
      <rectRegion x="66.98900" y="0.00000" w="33.01100" h="23.25400" t="6:31.687"/>
      <rectRegion x="66.98900" y="0.00000" w="33.01100" h="23.25400" t="6:38.200"/>
