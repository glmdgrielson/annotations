  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.15800" y="0.00000" w="29.84200" h="28.04200" t="1:40.185"/>
      <rectRegion x="70.15800" y="0.00000" w="29.84200" h="28.04200" t="1:46.700"/>
