    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="19.87300" t="2:27.266"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="19.87300" t="2:32.800"/>
