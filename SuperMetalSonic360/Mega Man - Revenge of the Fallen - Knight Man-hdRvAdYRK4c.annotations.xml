  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_782306011" type="text" style="popup" log_data="a-v=hdRvAdYRK4c&amp;xble=1&amp;a-type=1&amp;ei=ObEfXIHSM-av8gSw1o-YAg&amp;a-id=annotation_782306011" itct="">
