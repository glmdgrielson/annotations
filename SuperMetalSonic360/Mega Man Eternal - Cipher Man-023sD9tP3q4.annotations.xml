<annotation id="annotation_1120002489" type="text" style="popup" log_data="xble=1&amp;ei=OpofXIaIDIjxhgbDiInQBQ&amp;a-v=023sD9tP3q4&amp;a-id=annotation_1120002489&amp;a-type=1" itct="CAEQs-UBGP___________wEiEwiGjcq4k7bfAhWIuMEKHUNEAloo-B0yAml2SK69v9r9gfu20wE">
