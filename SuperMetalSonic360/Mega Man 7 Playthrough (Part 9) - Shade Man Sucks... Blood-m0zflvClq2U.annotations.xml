    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.55600" t="0:07:42.5"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.55600" t="0:07:47.5"/>
