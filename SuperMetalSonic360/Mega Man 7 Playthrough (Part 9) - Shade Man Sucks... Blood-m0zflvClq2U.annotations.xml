<annotation id="annotation_129436" type="text" style="popup" log_data="xble=1&amp;a-v=m0zflvClq2U&amp;a-type=1&amp;a-id=annotation_129436&amp;ei=Jk8gXJ67OMe3hwbctKfYCg" itct="CAIQs-UBGP___________wEiEwie5o7-v7ffAhXH28EKHVzaCaso-B0yAml2SOXWloXv8remmwE">
  <TEXT>This game sure is very heavy on dialogue. Much more the preceding games.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.55600" t="0:13:34.3"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.55600" t="0:13:39.3"/>
