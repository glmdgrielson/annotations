<annotation id="annotation_179274" type="text" style="popup" log_data="ei=1FEgXKayHsqBhgbp_bLIAw&amp;xble=1&amp;a-id=annotation_179274&amp;a-type=1&amp;a-v=9pP7-v2x-d4" itct="CAUQs-UBGP___________wEiEwim7ILFwrffAhXKgMEKHem-DDko-B0yAml2SN7zx-2v__7J9gE">
