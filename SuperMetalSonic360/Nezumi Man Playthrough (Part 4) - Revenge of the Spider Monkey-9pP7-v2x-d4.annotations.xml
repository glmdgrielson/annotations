    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="28.88900" t="0:00:29.2"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="28.88900" t="0:00:34.2"/>
