    <movingRegion type="rect">
      <rectRegion x="70.37000" y="0.00000" w="29.63000" h="24.16700" t="6:47.765"/>
      <rectRegion x="70.37000" y="0.00000" w="29.63000" h="24.16700" t="6:52.800"/>
