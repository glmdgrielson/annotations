  <TEXT>&quot;Don&#39;t come near me, you&#39;ll find out what an easy boss I am!&quot;</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.27800" w="30.00000" h="16.94400" t="6:34.219"/>
      <rectRegion x="70.00000" y="0.27800" w="30.00000" h="16.94400" t="6:38.200"/>
