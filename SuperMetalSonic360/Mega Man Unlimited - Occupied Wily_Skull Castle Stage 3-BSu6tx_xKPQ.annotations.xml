    <movingRegion type="rect">
      <rectRegion x="69.07400" y="0.00000" w="30.92600" h="28.61100" t="3:15.669"/>
      <rectRegion x="69.07400" y="0.00000" w="30.92600" h="28.61100" t="3:21.700"/>
