  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_3809587081" type="text" style="popup" log_data="ei=f0MgXNOUNMi58gTC4qj4DA&amp;a-id=annotation_3809587081&amp;xble=1&amp;a-type=1&amp;a-v=BSu6tx_xKPQ" itct="">
  <TEXT>And now we enter the loopy realms of VVVVV. Yep, this is the gravity-flipping stage.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.81500" y="0.00000" w="30.18500" h="16.66700" t="0:46.180"/>
      <rectRegion x="69.81500" y="0.00000" w="30.18500" h="16.66700" t="0:51.200"/>
