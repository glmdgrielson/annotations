  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_1674076809" type="text" style="popup" log_data="ei=f0MgXNOUNMi58gTC4qj4DA&amp;a-id=annotation_1674076809&amp;xble=1&amp;a-type=1&amp;a-v=BSu6tx_xKPQ" itct="CAUQs-UBGP___________wEiEwiT0NbvtLffAhXInJwKHUIxCs8o-B0yAml2SPTRxP_x1u6VBQ">
  <TEXT>If you want to finish him quicker, then use Yoku Attack. It really burns through his health.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.37000" y="0.00000" w="29.63000" h="19.72200" t="7:18.999"/>
      <rectRegion x="70.37000" y="0.00000" w="29.63000" h="19.72200" t="7:24.000"/>
