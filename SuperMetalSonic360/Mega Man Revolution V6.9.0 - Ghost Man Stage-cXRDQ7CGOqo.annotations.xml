  <TEXT>This guy probably has my favourite design of this bunch. The dangling wires just look cool.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="24.72200" t="0:00:07.7"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="24.72200" t="0:00:12.7"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.25" bgColor="0" fgColor="16777215" textSize="3.30980833333333" effects="textdropshadow">
