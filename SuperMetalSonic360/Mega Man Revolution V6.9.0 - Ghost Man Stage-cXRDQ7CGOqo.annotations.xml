    <padding top="1.98588502407" left="1.48941373825" bottom="1.98588502407" right="1.98588502407"/>
  </appearance>
  
</annotation>
<annotation id="annotation_22910" type="text" style="popup" log_data="ei=7UUgXNCMDcfHhwaX25iQCg&amp;a-id=annotation_22910&amp;a-type=1&amp;a-v=cXRDQ7CGOqo&amp;xble=1" itct="CAQQs-UBGP___________wEiEwiQt_uXt7ffAhXH48EKHZctBqIo-B0yAml2SKr1mIS76JC6cQ">
  <TEXT>This stage is absolutely full of split pathways. This set is a normal split pathway, but dead ends crop up later.</TEXT>
