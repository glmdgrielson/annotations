  </segment>
  <appearance bgAlpha="0.25" bgColor="0" fgColor="16777215" textSize="3.30980833333333" effects="textdropshadow">
    <padding top="1.98588502407" left="1.48941373825" bottom="1.98588502407" right="1.98588502407"/>
  </appearance>
  
</annotation>
<annotation id="annotation_225141" type="text" style="popup" log_data="ei=7UUgXNCMDcfHhwaX25iQCg&amp;a-id=annotation_225141&amp;a-type=1&amp;a-v=cXRDQ7CGOqo&amp;xble=1" itct="CAMQs-UBGP___________wEiEwiQt_uXt7ffAhXH48EKHZctBqIo-B0yAml2SKr1mIS76JC6cQ">
