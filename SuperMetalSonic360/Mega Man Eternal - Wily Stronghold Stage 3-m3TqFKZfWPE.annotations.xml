  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.15800" y="0.00000" w="29.84200" h="17.05600" t="2:36.761"/>
      <rectRegion x="70.15800" y="0.00000" w="29.84200" h="17.05600" t="2:41.300"/>
