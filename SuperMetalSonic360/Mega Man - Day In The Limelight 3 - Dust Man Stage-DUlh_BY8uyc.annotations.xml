<annotation id="annotation_1017085471" type="text" style="popup" log_data="a-v=DUlh_BY8uyc&amp;a-type=1&amp;xble=1&amp;a-id=annotation_1017085471&amp;ei=VD8gXMyqKojxhgao1JNA" itct="CAEQs-UBGP___________wEiEwjMpOjysLffAhWIuMEKHSjqBAgo-B0yAml2SKf28rHBv9ikDQ">
  <TEXT>This is a cool spin. These platforms now need to be broken apart with either jumping on them or under them.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.81500" y="0.00000" w="30.18500" h="20.00000" t="1:51.518"/>
      <rectRegion x="69.81500" y="0.00000" w="30.18500" h="20.00000" t="1:56.500"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
