<annotation id="annotation_998838459" type="text" style="popup" log_data="xble=1&amp;a-type=1&amp;a-v=BxWZd2VoS2E&amp;ei=DZYfXILkOMyJhwbm_buwBQ&amp;a-id=annotation_998838459" itct="CAkQs-UBGP___________wEiEwjCnpi7j7bfAhXMxMEKHeb-DlYo-B0yAml2SOGWoav2ruaKBw">
