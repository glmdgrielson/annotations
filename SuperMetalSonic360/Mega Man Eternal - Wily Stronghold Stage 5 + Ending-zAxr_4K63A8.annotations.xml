    <movingRegion type="rect">
      <rectRegion x="18.54200" y="72.95800" w="62.16300" h="12.54900" t="1:31.100"/>
      <rectRegion x="18.54200" y="72.95800" w="62.16300" h="12.54900" t="1:37.600"/>
