  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_3181057297" type="text" style="popup" log_data="a-v=pbDhJg7aaRA&amp;a-id=annotation_3181057297&amp;ei=oDQgXITfHNKuhwbv7aegDg&amp;xble=1&amp;a-type=1" itct="CAgQs-UBGP___________wEiEwiEr5bYprffAhVS18EKHe_2CeQo-B0yAml2SJDS6fbgpLjYpQE">
  <TEXT>This is the kind of difficult that I like. It isn&#39;t random, unfair or cheap, and rewards you for learning the fight.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.04900" y="0.00000" w="30.95100" h="21.28200" t="0:25.698"/>
      <rectRegion x="69.04900" y="0.00000" w="30.95100" h="21.28200" t="0:31.200"/>
