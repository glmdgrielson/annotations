  <TEXT>Okuu Rocket Dive Go!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="44.21600" y="32.95800" w="30.00000" h="8.88900" t="2:00.776"/>
      <rectRegion x="44.21600" y="32.95800" w="30.00000" h="8.88900" t="2:04.300"/>
