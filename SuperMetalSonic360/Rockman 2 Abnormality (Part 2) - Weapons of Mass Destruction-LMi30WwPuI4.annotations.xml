    <movingRegion type="rect">
      <rectRegion x="70.79200" y="0.00000" w="29.20800" h="28.32400" t="10:22.523"/>
      <rectRegion x="70.79200" y="0.00000" w="29.20800" h="28.32400" t="10:30.000"/>
