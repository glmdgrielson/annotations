  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.52500" y="0.00000" w="30.47500" h="21.00000" t="6:44.423"/>
      <rectRegion x="69.52500" y="0.00000" w="30.47500" h="21.00000" t="6:49.900"/>
