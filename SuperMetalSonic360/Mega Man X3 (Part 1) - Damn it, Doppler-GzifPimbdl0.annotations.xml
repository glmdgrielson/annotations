    <movingRegion type="rect">
      <rectRegion x="69.52500" y="0.00000" w="30.47500" h="20.43700" t="0:14.436"/>
      <rectRegion x="69.52500" y="0.00000" w="30.47500" h="20.43700" t="0:19.900"/>
