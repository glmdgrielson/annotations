<annotation id="annotation_2320877231" type="text" style="popup" log_data="a-id=annotation_2320877231&amp;a-type=1&amp;a-v=GzifPimbdl0&amp;ei=cCsgXP3WCsTS8gS2pKiAAw&amp;xble=1" itct="CAUQs-UBGP___________wEiEwj9zsH2nbffAhVEqZwKHTYSCjAo-B0yAml2SN3s7czi56ecGw">
  <TEXT>This must be where the inspiration for Freudenstachel&#39;s intro stage came from. Anyway, introducing Zero.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.15500" t="4:44.331"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.15500" t="4:49.800"/>
