</annotation>
<annotation id="annotation_282399" type="text" style="popup" log_data="a-type=1&amp;xble=1&amp;a-id=annotation_282399&amp;a-v=TYFEtiA_1pg&amp;ei=fUQgXI-QDNLV8wS_9q-ABw" itct="CAoQs-UBGP___________wEiEwjPwr3otbffAhXS6pwKHT_7C3Ao-B0yAml2SJit_4HiltHATQ">
  <TEXT>This form is also pretty simple. Just start charging when he is about 2/3 across the screen.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.55600" t="0:03:13.4"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="20.55600" t="0:03:18.4"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.30980833333333" effects="textdropshadow">
