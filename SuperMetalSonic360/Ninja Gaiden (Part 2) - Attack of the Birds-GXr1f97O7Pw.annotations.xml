<annotation id="annotation_3100262753" type="text" style="popup" log_data="a-v=GXr1f97O7Pw&amp;a-id=annotation_3100262753&amp;a-type=1&amp;xble=1&amp;ei=UjUgXIubDofuhgaDmpngBg" itct="CAQQs-UBGP___________wEiEwiLjPisp7ffAhUHt8EKHQNNBmwo-B0yAml2SPzZu_b9r729GQ">
  <TEXT>Here we fight Kelbeross. The quick one is the one you want to attack. If you need to, take cover under the pillars, they can&#39;t reach you there.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="24.38000" t="9:03.235"/>
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="24.38000" t="9:09.700"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
