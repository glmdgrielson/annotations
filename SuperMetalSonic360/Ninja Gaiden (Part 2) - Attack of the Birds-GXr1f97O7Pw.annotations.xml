  <TEXT>Not a good time to say this, but I may have lost my keys.</TEXT>
  <segment>
    <movingRegion type="anchored">
      <anchoredRegion x="14.42200" y="29.01400" w="31.06200" h="13.67800" sx="29.0049991608" sy="44.6920013428" t="6:53.595"/>
      <anchoredRegion x="14.42200" y="29.01400" w="31.06200" h="13.67800" sx="29.0049991608" sy="44.6920013428" t="6:58.100"/>
