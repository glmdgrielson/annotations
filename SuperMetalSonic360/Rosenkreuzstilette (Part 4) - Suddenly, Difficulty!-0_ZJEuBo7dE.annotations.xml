  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_3625388097" type="text" style="popup" log_data="a-v=0_ZJEuBo7dE&amp;a-type=1&amp;xble=1&amp;a-id=annotation_3625388097&amp;ei=ED0gXM3nD5e0hwa56pKwBA" itct="CAgQs-UBGP___________wEiEwjNr4XerrffAhUX2sEKHTm1BEYo-B0yAml2SNHbo4OuopL70wE">
  <TEXT>This part here is very similar to Flash Man. Likewise, if you fall, you have to deal with more enemies.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.47500" y="0.00000" w="29.52500" h="19.87300" t="1:55.129"/>
      <rectRegion x="70.47500" y="0.00000" w="29.52500" h="19.87300" t="2:00.600"/>
