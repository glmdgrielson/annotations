    <movingRegion type="rect">
      <rectRegion x="69.84200" y="0.00000" w="30.15800" h="21.00000" t="2:05.825"/>
      <rectRegion x="69.84200" y="0.00000" w="30.15800" h="21.00000" t="2:11.300"/>
