  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.84200" y="0.00000" w="30.15800" h="23.81700" t="6:40.296"/>
      <rectRegion x="69.84200" y="0.00000" w="30.15800" h="23.81700" t="6:45.800"/>
