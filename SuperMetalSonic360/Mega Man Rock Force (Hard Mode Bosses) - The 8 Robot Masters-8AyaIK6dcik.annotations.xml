<annotation id="annotation_2198403839" type="text" style="popup" log_data="a-id=annotation_2198403839&amp;a-type=1&amp;a-v=8AyaIK6dcik&amp;ei=SjUgXImZI4eKhwal6pe4Bw&amp;l-class=1&amp;link=http%3A%2F%2Fwww.youtube.com%2Fwatch%3Fv%3DaVfKRuw_I78%26list%3DPLX8RuKfg96JSv_M1VGeVbH8cPo5F4yQBZ%26index%3D1&amp;xble=1" itct="CAUQs-UBGP___________wEiEwiJ5qSpp7ffAhUHxcEKHSX1BXco-B0yAml2SKnk9fSKxKaG8AE">
