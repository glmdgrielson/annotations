<annotation id="annotation_2291581313" type="text" style="popup" log_data="ei=9BIgXJbYKaXv8gT_roewBg&amp;xble=1&amp;a-type=1&amp;a-id=annotation_2291581313&amp;a-v=Wr56GPSrkoI" itct="CAIQs-UBGP___________wEiEwiWovjJhrffAhWlt5wKHX_XAWYo-B0yAml2SIKlrqWPw57fWg">
  <TEXT>Hey! This is a kids game, watch the potty mouth.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="10.85900" t="5:24.397"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="10.85900" t="5:28.900"/>
