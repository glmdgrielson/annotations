<annotation id="annotation_224682165" type="text" style="popup" log_data="ei=TZAfXPjkFIiTgwO2zoiACQ&amp;a-id=annotation_224682165&amp;xble=1&amp;a-type=1&amp;a-v=VRhQ-RCkI5M" itct="CAEQs-UBGP___________wEiEwi4v4D9ibbfAhWIyWAKHTYnApAo-B0yAml2SJPHkIWRn5SMVQ">
  <TEXT>Lightning Flash is especially good for dealing with shielded enemies. It&#39;s excellent for clearing through large groups of enemies too.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="67.94000" y="0.00000" w="32.06000" h="21.56300" t="8:48.583"/>
      <rectRegion x="67.94000" y="0.00000" w="32.06000" h="21.56300" t="8:55.100"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
