<annotation id="annotation_3954481891" type="text" style="popup" log_data="ei=Hy4gXKy6F7SwhwaFtaaYBQ&amp;xble=1&amp;a-type=1&amp;a-id=annotation_3954481891&amp;a-v=bhJfNGcmg00" itct="CAYQs-UBGP___________wEiEwjsxZm-oLffAhU02MEKHYWaCVMo-B0yAml2SM2GmrnG5peJbg">
  <TEXT>This next area is essentially a test of patience. Unless you attempt to run through, nothing can really harm you.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.84200" y="0.00000" w="30.15800" h="19.59200" t="1:01.886"/>
      <rectRegion x="69.84200" y="0.00000" w="30.15800" h="19.59200" t="1:07.400"/>
