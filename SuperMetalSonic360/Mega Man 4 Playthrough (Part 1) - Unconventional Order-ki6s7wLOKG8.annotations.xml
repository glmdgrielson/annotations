<annotation id="annotation_135183" type="text" style="popup" log_data="a-type=1&amp;a-id=annotation_135183&amp;xble=1&amp;ei=1EwgXMvcCMPF8wS78rjgCQ&amp;a-v=ki6s7wLOKG8" itct="CAMQs-UBGP___________wEiEwjLlsDivbffAhXD4pwKHTs5Dpwo-B0yAml2SO_QuJbwnauXkgE">
