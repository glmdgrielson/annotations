  <appearance bgAlpha="0.25" bgColor="0" fgColor="16777215" textSize="3.30980833333333" effects="textdropshadow">
    <padding top="1.98588502407" left="1.48941373825" bottom="1.98588502407" right="1.98588502407"/>
  </appearance>
  
</annotation>
