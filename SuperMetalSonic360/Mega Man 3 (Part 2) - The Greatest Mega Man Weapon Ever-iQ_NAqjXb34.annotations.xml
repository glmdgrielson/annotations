  </appearance>
  
</annotation>
<annotation id="annotation_768566" type="text" style="popup" log_data="ei=10YgXLXxO8XEgwPu9pD4Bg&amp;xble=1&amp;a-v=iQ_NAqjXb34&amp;a-id=annotation_768566&amp;a-type=1" itct="">
  <TEXT>Gemini Laser, I choose you! Be awesome!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="16.38900" t="0:01:12.1"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="16.38900" t="0:01:17.1"/>
