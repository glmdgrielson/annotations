    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="24.72200" t="0:01:55.9"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="24.72200" t="0:02:00.9"/>
