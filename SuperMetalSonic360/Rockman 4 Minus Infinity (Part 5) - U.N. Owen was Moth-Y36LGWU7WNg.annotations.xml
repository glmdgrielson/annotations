    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="14.80300" t="4:51.763"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="14.80300" t="4:56.800"/>
