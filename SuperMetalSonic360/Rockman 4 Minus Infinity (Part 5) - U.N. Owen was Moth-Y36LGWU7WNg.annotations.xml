  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_2413959619" type="text" style="popup" log_data="a-type=1&amp;a-v=Y36LGWU7WNg&amp;xble=1&amp;ei=kT4gXNvNAZqohgabz77oDA&amp;a-id=annotation_2413959619" itct="">
  <TEXT>Welcome to Cossack Citadel, specifically, Blizzard Sky. I like how all this stages have individual names, it&#39;s a very nice touch.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.52500" y="0.00000" w="30.47500" h="18.74600" t="0:00.000"/>
      <rectRegion x="69.52500" y="0.00000" w="30.47500" h="18.74600" t="0:05.000"/>
