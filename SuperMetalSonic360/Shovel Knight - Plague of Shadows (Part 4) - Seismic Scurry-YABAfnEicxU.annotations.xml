  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_4102285665" type="text" style="popup" log_data="a-id=annotation_4102285665&amp;a-type=1&amp;xble=1&amp;a-v=YABAfnEicxU&amp;ei=f5EfXJvhGcu68wT2tY_wBA" itct="CAkQs-UBGP___________wEiEwjbnPqOi7bfAhVL3ZwKHfbaA04o-B0yAml2SJXmiYnnj5CAYA">
  <TEXT>I haven&#39;t found a safe way of taking these enemies out that doesn&#39;t include waiting around for them to get in your line of fire.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="68.89100" y="7.60600" w="31.10900" h="21.00000" t="7:24.969"/>
      <rectRegion x="68.89100" y="7.60600" w="31.10900" h="21.00000" t="7:31.500"/>
