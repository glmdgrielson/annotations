    <movingRegion type="rect">
      <rectRegion x="68.73200" y="7.32400" w="31.26800" h="21.00000" t="1:14.334"/>
      <rectRegion x="68.73200" y="7.32400" w="31.26800" h="21.00000" t="1:21.800"/>
