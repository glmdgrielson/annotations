  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_243799049" type="text" style="popup" log_data="a-id=annotation_243799049&amp;ei=qTkgXIfYEI7dhwaajq6gCg&amp;xble=1&amp;a-v=09b-_5YA8LA&amp;a-type=1" itct="CAcQs-UBGP___________wEiEwjH0Ny-q7ffAhWO7sEKHRqHC6Qo-B0yAml2SLDhg7D537_r0wE">
  <TEXT>This is actually where things can be dangerous. These missiles really overcrowd this room. Whatever you do, don&#39;t scroll them back on.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="23.81700" t="9:03.246"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="23.81700" t="9:08.700"/>
