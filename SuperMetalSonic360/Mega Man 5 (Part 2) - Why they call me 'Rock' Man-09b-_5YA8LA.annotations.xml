    <movingRegion type="rect">
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="16.77500" t="1:11.798"/>
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="16.77500" t="1:15.300"/>
