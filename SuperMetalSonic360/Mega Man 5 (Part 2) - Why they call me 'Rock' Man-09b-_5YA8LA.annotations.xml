    <movingRegion type="rect">
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="24.09900" t="0:19.747"/>
      <rectRegion x="70.00000" y="0.00000" w="30.00000" h="24.09900" t="0:26.200"/>
