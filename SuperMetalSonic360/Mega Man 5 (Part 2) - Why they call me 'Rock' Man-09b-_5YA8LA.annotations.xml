    <movingRegion type="rect">
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="21.28200" t="2:54.546"/>
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="21.28200" t="3:00.000"/>
