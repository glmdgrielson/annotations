  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="5670865" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_434661933" type="text" style="popup" log_data="ei=EjsgXPeZGoKNhgazrpfgAg&amp;a-id=annotation_434661933&amp;xble=1&amp;a-type=1&amp;a-v=PdbSaT24abY" itct="CAoQs-UBGP___________wEiEwj36vfqrLffAhWChsEKHTPXBSwo-B0yAml2SLbT4e2TzbTrPQ">
  <TEXT>This is where things do get a little sadistic. You can either shoot the enemy fast, or safely hop over it.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.36600" y="0.00000" w="30.63400" h="20.15500" t="6:34.246"/>
      <rectRegion x="69.36600" y="0.00000" w="30.63400" h="20.15500" t="6:39.700"/>
