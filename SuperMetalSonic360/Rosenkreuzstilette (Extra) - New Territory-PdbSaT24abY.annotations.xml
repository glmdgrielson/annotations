    <movingRegion type="rect">
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="25.50700" t="0:22.084"/>
      <rectRegion x="70.31700" y="0.00000" w="29.68300" h="25.50700" t="0:28.600"/>
