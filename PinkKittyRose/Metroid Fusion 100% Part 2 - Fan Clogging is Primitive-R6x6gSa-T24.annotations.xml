</annotation>
<annotation id="annotation_313190" type="text" style="popup" log_data="a-type=1&amp;a-id=annotation_313190&amp;a-v=R6x6gSa-T24&amp;ei=GlgeXOyJFJGnhwaMlJHgDA&amp;xble=1" itct="CAMQs-UBGP___________wEiEwjs_t6e4LPfAhWR08EKHQxKBMwo-B0yAml2SO6e-bWS0J7WRw">
  <TEXT>Oh gods... That objective sounds so painfully like spy games... XD</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="68.12500" y="0.00000" w="31.87500" h="11.66700" t="0:04:38.9"/>
      <rectRegion x="68.12500" y="0.00000" w="31.87500" h="11.66700" t="0:04:43.9"/>
