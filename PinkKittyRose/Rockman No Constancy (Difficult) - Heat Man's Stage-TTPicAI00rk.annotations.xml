    <movingRegion type="rect">
      <rectRegion x="36.45800" y="0.00000" w="63.54200" h="16.11100" t="0:02:23.4"/>
      <rectRegion x="36.45800" y="0.00000" w="63.54200" h="16.11100" t="0:02:30.4"/>
