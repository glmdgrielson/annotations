<annotation id="annotation_357029" type="text" style="popup" log_data="a-v=jr8p4vZMCCw&amp;xble=1&amp;a-id=annotation_357029&amp;a-type=1&amp;ei=6IodXIemJNLhhwbVv4X4AQ" itct="CAgQs-UBGP___________wEiEwiHus7GnLLfAhXS8MEKHdVfAR8o-B0yAml2SKyQsLKvvMrfjgE">
  <TEXT>That first enemy is your hint... Look before you leap in this room unless you want a jellyfish to the face.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="54.58300" y="0.00000" w="45.41700" h="16.11100" t="0:02:16.7"/>
      <rectRegion x="54.58300" y="0.00000" w="45.41700" h="16.11100" t="0:02:22.7"/>
