  <segment>
    <movingRegion type="rect">
      <rectRegion x="47.93200" y="16.38900" w="50.85200" h="23.88900" t="0:01:47.5"/>
      <rectRegion x="47.93200" y="16.38900" w="50.85200" h="23.88900" t="0:01:52.5"/>
