    <movingRegion type="rect">
      <rectRegion x="64.96500" y="4.78900" w="31.40500" h="15.54300" t="0:50.333"/>
      <rectRegion x="64.96500" y="4.78900" w="31.40500" h="15.54300" t="0:56.400"/>
