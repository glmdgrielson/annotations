  <TEXT>That raises the water to its highest level.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="67.07700" y="6.76100" w="24.88600" h="11.97900" t="7:49.370"/>
      <rectRegion x="67.07700" y="6.76100" w="24.88600" h="11.97900" t="7:54.400"/>
