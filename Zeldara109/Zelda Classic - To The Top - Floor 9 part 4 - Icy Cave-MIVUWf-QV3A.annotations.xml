<annotation id="annotation_3908587375" type="text" style="popup" log_data="a-id=annotation_3908587375&amp;a-type=1&amp;a-v=MIVUWf-QV3A&amp;xble=1&amp;ei=Bb43XNPEJKWa8gTUvq-wCQ" itct="">
  <TEXT>Time to explore this Icy Cave.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="66.90100" y="8.45100" w="30.81000" h="8.04200" t="0:02.439"/>
      <rectRegion x="66.90100" y="8.45100" w="30.81000" h="8.04200" t="0:08.400"/>
