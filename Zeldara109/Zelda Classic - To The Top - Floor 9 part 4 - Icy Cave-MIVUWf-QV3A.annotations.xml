    <movingRegion type="rect">
      <rectRegion x="61.09200" y="7.88700" w="39.08500" h="11.12700" t="7:10.600"/>
      <rectRegion x="61.09200" y="7.88700" w="39.08500" h="11.12700" t="7:16.600"/>
