    <movingRegion type="rect">
      <rectRegion x="64.78900" y="3.38000" w="33.27500" h="18.45100" t="8:47.670"/>
      <rectRegion x="64.78900" y="3.38000" w="33.27500" h="18.45100" t="8:53.700"/>
