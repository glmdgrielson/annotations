    <movingRegion type="rect">
      <rectRegion x="67.95800" y="2.53500" w="31.23200" h="15.07800" t="2:49.200"/>
      <rectRegion x="67.95800" y="2.53500" w="31.23200" h="15.07800" t="2:55.200"/>
