    <movingRegion type="rect">
      <rectRegion x="71.30300" y="6.47900" w="27.53500" h="12.26100" t="3:44.237"/>
      <rectRegion x="71.30300" y="6.47900" w="27.53500" h="12.26100" t="3:49.700"/>
