    <movingRegion type="rect">
      <rectRegion x="70.24600" y="0.84500" w="28.42400" h="19.85900" t="5:42.464"/>
      <rectRegion x="70.24600" y="0.84500" w="28.42400" h="19.85900" t="5:48.500"/>
