    <movingRegion type="rect">
      <rectRegion x="64.96500" y="5.07000" w="26.35400" h="15.55500" t="0:26.300"/>
      <rectRegion x="64.96500" y="5.07000" w="26.35400" h="15.55500" t="0:31.300"/>
