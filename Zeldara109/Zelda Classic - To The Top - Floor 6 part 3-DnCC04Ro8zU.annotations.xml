    <movingRegion type="rect">
      <rectRegion x="64.08500" y="3.38000" w="29.99100" h="19.01400" t="3:15.731"/>
      <rectRegion x="64.08500" y="3.38000" w="29.99100" h="19.01400" t="3:21.700"/>
