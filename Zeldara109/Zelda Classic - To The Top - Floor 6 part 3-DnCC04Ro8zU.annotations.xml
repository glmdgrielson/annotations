  <segment>
    <movingRegion type="rect">
      <rectRegion x="64.78900" y="2.81700" w="31.40000" h="19.01400" t="0:39.068"/>
      <rectRegion x="64.78900" y="2.81700" w="31.40000" h="19.01400" t="0:45.100"/>
