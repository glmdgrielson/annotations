  <TEXT>Switches can also be concealed...</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="64.78900" y="9.29600" w="34.75400" h="7.19000" t="4:10.075"/>
      <rectRegion x="64.78900" y="9.29600" w="34.75400" h="7.19000" t="4:15.100"/>
