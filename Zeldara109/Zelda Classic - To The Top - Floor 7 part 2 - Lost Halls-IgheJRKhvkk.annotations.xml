  <appearance bgAlpha="0.600000023842" bgColor="0" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_3900316885" type="text" style="popup" log_data="a-v=IgheJRKhvkk&amp;a-type=1&amp;xble=1&amp;a-id=annotation_3900316885&amp;ei=Er43XMeaLoT68wTm1LOgDw" itct="">
  <TEXT>In this raft path, depending on how the central moving whirlpool lines up, you may have to turn one tile early/late.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="65.49300" y="1.97200" w="31.40000" h="19.57700" t="2:41.054"/>
      <rectRegion x="65.49300" y="1.97200" w="31.40000" h="19.57700" t="2:47.100"/>
