  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.54200" y="6.76100" w="29.29600" h="11.97200" t="4:06.103"/>
      <rectRegion x="69.54200" y="6.76100" w="29.29600" h="11.97200" t="4:12.100"/>
