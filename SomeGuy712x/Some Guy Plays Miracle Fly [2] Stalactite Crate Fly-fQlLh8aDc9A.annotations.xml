  <TEXT>Awesome combo!
(8 kills, 18,000 points)</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="78.71600" y="11.54900" w="21.28400" h="10.84500" t="2:28.500"/>
      <rectRegion x="78.71600" y="11.54900" w="21.28400" h="10.84500" t="2:32.500"/>
