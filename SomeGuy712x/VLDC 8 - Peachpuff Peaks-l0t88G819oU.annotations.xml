  <TEXT>DOUBLE KILL!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="40.25400" y="78.02800" w="20.04200" h="6.62700" t="0:08.300"/>
      <rectRegion x="40.25400" y="78.02800" w="20.04200" h="6.62700" t="0:09.800"/>
