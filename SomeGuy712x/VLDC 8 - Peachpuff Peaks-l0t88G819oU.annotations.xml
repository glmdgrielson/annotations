<annotation id="annotation_2029847765" type="text" style="popup" log_data="xble=1&amp;a-type=1&amp;a-v=l0t88G819oU&amp;ei=zEMMXPzEF4j2NpyOvjA&amp;a-id=annotation_2029847765" itct="CAkQs-UBGP___________wEiEwj82pWsopHfAhUIuw0KHRyHDwYo-B0yAml2SIXt1_mGnt-llwE">
  <TEXT>Now I just have to wait for a nice opening in the Super Koopas...</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="11.44100" y="15.21100" w="76.36700" h="6.62700" t="3:00.500"/>
      <rectRegion x="11.44100" y="15.21100" w="76.36700" h="6.62700" t="3:04.500"/>
