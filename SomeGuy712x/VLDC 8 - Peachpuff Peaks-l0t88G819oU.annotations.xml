    <movingRegion type="rect">
      <rectRegion x="22.66900" y="93.52100" w="55.42400" h="6.62700" t="3:21.038"/>
      <rectRegion x="22.66900" y="93.52100" w="55.42400" h="6.62700" t="3:25.000"/>
