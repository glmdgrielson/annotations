<annotation id="annotation_2773209129" type="text" style="popup" log_data="ei=a-ULXMvDPMmojQTiyIT4BQ&amp;a-id=annotation_2773209129&amp;xble=1&amp;a-type=1&amp;a-v=T5sOWXh-e84" itct="">
  <TEXT>And, there we go!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="42.63100" y="93.52100" w="17.15600" h="6.62700" t="6:28.000"/>
      <rectRegion x="42.63100" y="93.52100" w="17.15600" h="6.62700" t="6:31.000"/>
