    <movingRegion type="rect">
      <rectRegion x="30.42800" y="11.54900" w="38.39200" h="6.62700" t="0:26.500"/>
      <rectRegion x="30.42800" y="11.54900" w="38.39200" h="6.62700" t="0:30.500"/>
