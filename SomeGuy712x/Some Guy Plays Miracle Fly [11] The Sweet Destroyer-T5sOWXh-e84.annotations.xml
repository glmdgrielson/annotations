  <TEXT>W-W-W-W-WHOA!!!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="41.83800" y="93.52100" w="18.74100" h="6.62700" t="5:50.500"/>
      <rectRegion x="41.83800" y="93.52100" w="18.74100" h="6.62700" t="5:53.500"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="0" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
