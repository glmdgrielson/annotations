  <appearance bgAlpha="0.600000023842" bgColor="0" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_3507630165" type="text" style="popup" log_data="ei=a-ULXMvDPMmojQTiyIT4BQ&amp;a-id=annotation_3507630165&amp;xble=1&amp;a-type=1&amp;a-v=T5sOWXh-e84" itct="">
  <TEXT>To be continued... Yo!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="39.30300" y="93.52100" w="20.64300" h="6.62700" t="7:50.500"/>
      <rectRegion x="39.30300" y="93.52100" w="20.64300" h="6.62700" t="7:53.500"/>
