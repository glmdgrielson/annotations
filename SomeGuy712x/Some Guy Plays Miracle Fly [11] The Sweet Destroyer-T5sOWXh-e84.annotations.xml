<annotation id="annotation_3527465239" type="text" style="popup" log_data="ei=a-ULXMvDPMmojQTiyIT4BQ&amp;a-id=annotation_3527465239&amp;xble=1&amp;a-type=1&amp;a-v=T5sOWXh-e84" itct="">
  <TEXT>Alright, it&#39;s boss time!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="41.04600" y="93.52100" w="20.48400" h="6.62700" t="0:56.500"/>
      <rectRegion x="41.04600" y="93.52100" w="20.48400" h="6.62700" t="0:59.500"/>
