    <movingRegion type="rect">
      <rectRegion x="3.95100" y="89.15500" w="91.71600" h="10.84500" t="0:14.500"/>
      <rectRegion x="3.95100" y="89.15500" w="91.71600" h="10.84500" t="0:20.500"/>
