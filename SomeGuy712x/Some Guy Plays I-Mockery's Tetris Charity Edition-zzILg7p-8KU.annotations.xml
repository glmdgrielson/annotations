    <movingRegion type="rect">
      <rectRegion x="9.13600" y="89.01400" w="80.11600" h="11.13400" t="7:11.500"/>
      <rectRegion x="9.13600" y="89.01400" w="80.11600" h="11.13400" t="7:16.500"/>
