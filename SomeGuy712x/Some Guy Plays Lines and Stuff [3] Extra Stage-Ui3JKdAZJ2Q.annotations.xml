<annotation id="annotation_56907" type="text" style="popup" log_data="a-id=annotation_56907&amp;a-type=1&amp;a-v=Ui3JKdAZJ2Q&amp;xble=1&amp;ei=SLQMXPiYAtrvigSgwpPwDQ" itct="">
  <TEXT>...? blocks appear up here!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="33.54200" y="92.77800" w="35.00000" h="7.22200" t="00:01:11.0"/>
      <rectRegion x="33.54200" y="92.77800" w="35.00000" h="7.22200" t="00:01:13.0"/>
