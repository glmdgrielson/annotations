    <movingRegion type="rect">
      <rectRegion x="12.44800" y="72.50000" w="76.97100" h="12.22200" t="0:03:37.5"/>
      <rectRegion x="12.44800" y="72.50000" w="76.97100" h="12.22200" t="0:03:42.5"/>
