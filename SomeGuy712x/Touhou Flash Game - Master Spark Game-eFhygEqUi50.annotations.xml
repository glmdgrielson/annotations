<annotation id="annotation_375569" type="text" style="popup" log_data="ei=Ma4MXOT0DqKBjASf7KnYBA&amp;a-id=annotation_375569&amp;xble=1&amp;a-type=1&amp;a-v=eFhygEqUi50" itct="CAgQs-UBGP___________wEiEwik0dvnh5LfAhWiAIMKHR92Ckso-B0yAml2SJ2X0tSE0JyseA">
