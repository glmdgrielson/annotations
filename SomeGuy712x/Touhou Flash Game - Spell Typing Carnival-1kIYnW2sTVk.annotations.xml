    <padding top="1.98588502407" left="1.48941373825" bottom="1.98588502407" right="1.98588502407"/>
  </appearance>
  
</annotation>
<annotation id="annotation_737675" type="text" style="popup" log_data="a-id=annotation_737675&amp;a-type=1&amp;xble=1&amp;a-v=1kIYnW2sTVk&amp;ei=CpQMXJO3M8OIjATYurmADg" itct="">
  <TEXT>Stage 2 pits us against Tewi. How complicated is her vocabulary? Let&#39;s find out!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="94.79200" h="8.05600" t="0:03:19.5"/>
      <rectRegion x="0.00000" y="0.00000" w="94.79200" h="8.05600" t="0:03:23.5"/>
