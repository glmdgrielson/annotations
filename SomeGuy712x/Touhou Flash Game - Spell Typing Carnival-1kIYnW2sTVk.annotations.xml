<annotation id="annotation_874511" type="text" style="popup" log_data="a-id=annotation_874511&amp;a-type=1&amp;xble=1&amp;a-v=1kIYnW2sTVk&amp;ei=CpQMXJO3M8OIjATYurmADg" itct="">
  <TEXT>I have no idea what this says...</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="30.41700" y="91.94400" w="39.58300" h="8.05600" t="0:01:08.0"/>
