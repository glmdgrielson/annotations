<annotation id="annotation_395702275" type="text" style="popup" log_data="xble=1&amp;ei=WA0MXNrEO4P1Npn7tNgE&amp;a-v=rmREn6yUWEM&amp;a-type=1&amp;a-id=annotation_395702275" itct="">
  <TEXT>Now, over on the right is a new obstacle.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="30.42800" y="93.52100" w="37.12500" h="6.62700" t="3:13.500"/>
      <rectRegion x="30.42800" y="93.52100" w="37.12500" h="6.62700" t="3:17.500"/>
