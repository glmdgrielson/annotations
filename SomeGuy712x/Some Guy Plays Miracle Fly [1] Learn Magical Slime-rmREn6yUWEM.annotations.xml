    <movingRegion type="rect">
      <rectRegion x="22.50400" y="89.29600" w="59.62900" h="10.85200" t="5:42.500"/>
      <rectRegion x="22.50400" y="89.29600" w="59.62900" h="10.85200" t="5:48.500"/>
