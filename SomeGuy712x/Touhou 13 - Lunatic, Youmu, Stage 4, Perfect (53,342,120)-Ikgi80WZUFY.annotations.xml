  <TEXT>Demo count: 5</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="78.54200" y="87.22200" w="21.45800" h="8.05600" t="0:03:19.0"/>
      <rectRegion x="78.54200" y="87.22200" w="21.45800" h="8.05600" t="0:03:23.0"/>
