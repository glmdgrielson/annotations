</annotation>
<annotation id="annotation_346266" type="text" style="popup" log_data="a-id=annotation_346266&amp;a-type=1&amp;a-v=_G56sSqi_tU&amp;xble=1&amp;ei=yKkMXPPdJ5ibigSR3rewBQ" itct="">
  <TEXT>Now, let&#39;s go over the other selections here...</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="26.74800" y="0.00000" w="47.37800" h="8.05600" t="0:03:38.5"/>
      <rectRegion x="26.74800" y="0.00000" w="47.37800" h="8.05600" t="0:03:42.5"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="0" fgColor="16777215" textSize="3.30980833333333" effects="textdropshadow">
