    <movingRegion type="rect">
      <rectRegion x="11.45800" y="93.61100" w="77.29200" h="6.66600" t="5:49.500"/>
      <rectRegion x="11.45800" y="93.61100" w="77.29200" h="6.66600" t="5:53.500"/>
