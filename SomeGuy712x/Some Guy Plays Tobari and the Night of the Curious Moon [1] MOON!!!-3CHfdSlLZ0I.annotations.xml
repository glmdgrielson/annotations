  <TEXT>Going to collection... Well, I kinda haven&#39;t gotten anything there yet.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="59.16700" y="0.00000" w="40.83300" h="10.83300" t="0:39.500"/>
      <rectRegion x="59.16700" y="0.00000" w="40.83300" h="10.83300" t="0:43.500"/>
