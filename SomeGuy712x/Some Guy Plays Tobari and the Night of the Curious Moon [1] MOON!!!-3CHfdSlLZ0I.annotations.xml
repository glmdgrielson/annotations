    <movingRegion type="rect">
      <rectRegion x="23.33300" y="89.44400" w="53.54200" h="10.83300" t="7:21.000"/>
      <rectRegion x="23.33300" y="89.44400" w="53.54200" h="10.83300" t="7:26.000"/>
