  <segment>
    <movingRegion type="rect">
      <rectRegion x="40.83300" y="93.61100" w="20.41700" h="6.66600" t="6:36.500"/>
      <rectRegion x="40.83300" y="93.61100" w="20.41700" h="6.66600" t="6:39.500"/>
