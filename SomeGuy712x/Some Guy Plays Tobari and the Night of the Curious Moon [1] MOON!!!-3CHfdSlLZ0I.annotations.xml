  <TEXT>CHECKPOINT! I assume you respawn there if you die after hitting it.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="10.62500" y="93.61100" w="79.58300" h="6.65300" t="3:41.000"/>
      <rectRegion x="10.62500" y="93.61100" w="79.58300" h="6.65300" t="3:45.000"/>
