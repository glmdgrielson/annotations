  <TEXT>Okay, here we go!!!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="74.11300" y="92.77800" w="25.88700" h="7.22200" t="0:04:52.0"/>
      <rectRegion x="74.11300" y="92.77800" w="25.88700" h="7.22200" t="0:04:55.0"/>
