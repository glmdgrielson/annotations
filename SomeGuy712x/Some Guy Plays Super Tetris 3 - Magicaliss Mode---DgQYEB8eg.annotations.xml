  <TEXT>Oops... Now how should I deal with this?</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="69.72900" y="88.33300" w="30.27100" h="11.66700" t="0:03:10.5"/>
      <rectRegion x="69.72900" y="88.33300" w="30.27100" h="11.66700" t="0:03:13.5"/>
