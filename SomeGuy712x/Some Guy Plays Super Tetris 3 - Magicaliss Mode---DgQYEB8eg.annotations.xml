  <TEXT>That ought to help.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="74.73900" y="92.77800" w="25.26100" h="7.22200" t="0:03:49.5"/>
      <rectRegion x="74.73900" y="92.77800" w="25.26100" h="7.22200" t="0:03:52.5"/>
