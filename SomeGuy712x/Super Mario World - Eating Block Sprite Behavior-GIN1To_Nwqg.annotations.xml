    <movingRegion type="rect">
      <rectRegion x="69.52000" y="16.94400" w="30.48000" h="7.22200" t="0:04:49.5"/>
      <rectRegion x="69.52000" y="16.94400" w="30.48000" h="7.22200" t="0:04:52.5"/>
