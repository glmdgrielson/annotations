  <appearance bgAlpha="0.600000023842" bgColor="0" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_2129640545" type="text" style="popup" log_data="a-type=1&amp;xble=1&amp;a-id=annotation_2129640545&amp;a-v=UgPEmmQLBNM&amp;ei=GyQMXNCiIoLbwQHjh6KADQ" itct="">
  <TEXT>Well, I succeeded.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="38.33300" y="93.61100" w="23.95800" h="6.66600" t="6:37.000"/>
      <rectRegion x="38.33300" y="93.61100" w="23.95800" h="6.66600" t="6:41.000"/>
