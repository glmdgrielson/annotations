  <TEXT>Ooh, there&#39;s some new magic to get here!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="23.12500" y="93.61100" w="51.04200" h="6.66600" t="2:23.000"/>
      <rectRegion x="23.12500" y="93.61100" w="51.04200" h="6.66600" t="2:27.000"/>
