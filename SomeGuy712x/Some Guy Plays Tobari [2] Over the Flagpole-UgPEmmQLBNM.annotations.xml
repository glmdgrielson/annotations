  <TEXT>3 hits.</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="90.00000" y="0.00000" w="10.00000" h="6.65300" t="5:54.100"/>
      <rectRegion x="90.00000" y="0.00000" w="10.00000" h="6.65300" t="5:55.900"/>
