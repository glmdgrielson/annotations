    <movingRegion type="rect">
      <rectRegion x="13.99000" y="83.61100" w="75.04000" h="16.38900" t="0:02:56.0"/>
      <rectRegion x="13.99000" y="83.61100" w="75.04000" h="16.38900" t="0:03:04.0"/>
