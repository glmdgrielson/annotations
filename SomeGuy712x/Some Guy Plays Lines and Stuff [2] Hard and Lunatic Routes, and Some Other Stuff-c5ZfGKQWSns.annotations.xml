  <TEXT>Alright, so this time, I want to take the hard route!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="34.79200" y="88.33300" w="35.00000" h="11.66700" t="0:00:41.5"/>
      <rectRegion x="34.79200" y="88.33300" w="35.00000" h="11.66700" t="0:00:44.5"/>
