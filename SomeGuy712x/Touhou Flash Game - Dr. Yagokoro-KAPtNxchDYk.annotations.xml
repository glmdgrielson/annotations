  <TEXT>And I&#39;ve hit level 60. This is getting intense!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="59.44400" y="87.77800" w="40.55600" h="12.22200" t="0:06:04.0"/>
      <rectRegion x="59.44400" y="87.77800" w="40.55600" h="12.22200" t="0:06:08.0"/>
