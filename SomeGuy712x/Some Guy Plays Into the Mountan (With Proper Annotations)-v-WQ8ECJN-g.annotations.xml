    <movingRegion type="rect">
      <rectRegion x="18.75000" y="15.00000" w="61.66700" h="10.83300" t="1:11.500"/>
      <rectRegion x="18.75000" y="15.00000" w="61.66700" h="10.83300" t="1:16.500"/>
