  <TEXT>Hello, and welcome to a special short episode of Project Nite Festival!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="45.62500" h="10.83300" t="0:01.027"/>
      <rectRegion x="0.00000" y="0.00000" w="45.62500" h="10.83300" t="0:04.027"/>
