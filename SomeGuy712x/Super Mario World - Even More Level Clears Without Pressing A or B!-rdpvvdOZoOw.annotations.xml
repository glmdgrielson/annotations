    <movingRegion type="rect">
      <rectRegion x="15.20800" y="88.33300" w="70.62500" h="11.66700" t="0:06:07.5"/>
      <rectRegion x="15.20800" y="88.33300" w="70.62500" h="11.66700" t="0:06:11.5"/>
