    <movingRegion type="rect">
      <rectRegion x="22.96500" y="88.33300" w="53.23600" h="11.66700" t="0:00:16.5"/>
      <rectRegion x="22.96500" y="88.33300" w="53.23600" h="11.66700" t="0:00:20.5"/>
