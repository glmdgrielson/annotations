  <TEXT>Alright, it&#39;s boss time!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="68.54200" y="46.66700" w="27.08300" h="6.66600" t="2:43.000"/>
      <rectRegion x="68.54200" y="46.66700" w="27.08300" h="6.66600" t="2:46.000"/>
