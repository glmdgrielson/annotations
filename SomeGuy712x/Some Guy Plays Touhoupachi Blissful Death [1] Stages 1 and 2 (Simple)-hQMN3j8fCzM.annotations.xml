    <movingRegion type="rect">
      <rectRegion x="71.25000" y="46.94400" w="20.62500" h="27.50000" t="5:15.500"/>
      <rectRegion x="71.25000" y="46.94400" w="20.62500" h="27.50000" t="5:20.500"/>
