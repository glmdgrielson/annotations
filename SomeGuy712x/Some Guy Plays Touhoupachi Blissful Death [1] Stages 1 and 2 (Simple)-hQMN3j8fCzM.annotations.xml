  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="0" fgColor="16777215" textSize="3.3098083333333332" effects="">
    
  </appearance>
  
</annotation>
<annotation id="annotation_523899023" type="text" style="popup" log_data="a-v=hQMN3j8fCzM&amp;a-id=annotation_523899023&amp;ei=pVcMXI3UJcnXigTs-oaYDA&amp;xble=1&amp;a-type=1" itct="">
  <TEXT>BGM: ???</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="13.95800" h="6.66600" t="1:25.000"/>
      <rectRegion x="0.00000" y="0.00000" w="13.95800" h="6.66600" t="1:30.000"/>
