<?xml version="1.0" encoding="UTF-8" ?><document><annotations itct="CAAQwOYBIhMIxPKB_7C03wIVIuQ4Ch3lGw6pKPgdMgJpdkjQpY-lsZPzmN8B">
<annotation id="annotation_1556776277" type="text" style="popup" log_data="ei=x6weXITzA6LI4wHlt7jICg&amp;a-id=annotation_1556776277&amp;a-type=1&amp;a-v=3zHMmxSj0tA&amp;xble=1" itct="CAEQs-UBGP___________wEiEwjE8oH_sLTfAhUi5DgKHeUbDqko-B0yAml2SNClj6Wxk_OY3wE">
