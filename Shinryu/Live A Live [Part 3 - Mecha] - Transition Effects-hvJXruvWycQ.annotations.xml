    <movingRegion type="rect">
      <rectRegion x="0.00000" y="81.94400" w="100.00000" h="16.95800" t="0:18.161"/>
      <rectRegion x="0.00000" y="81.94400" w="100.00000" h="16.95800" t="0:25.400"/>
    </movingRegion>
  </segment>
  <appearance bgAlpha="0.600000023842" bgColor="0" fgColor="16777215" textSize="3.911599999999999" effects="">
    
  </appearance>
  
</annotation>
