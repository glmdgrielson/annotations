  <segment>
    <movingRegion type="rect">
      <rectRegion x="0.00000" y="91.09700" w="30.24200" h="8.90300" t="17:54.133"/>
      <rectRegion x="0.00000" y="91.09700" w="30.24200" h="8.90300" t="17:57.000"/>
