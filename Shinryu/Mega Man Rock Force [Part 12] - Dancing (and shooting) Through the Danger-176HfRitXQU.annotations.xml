    <movingRegion type="rect">
      <rectRegion x="0.00000" y="89.01400" w="38.88900" h="7.47900" t="3:52.374"/>
      <rectRegion x="0.00000" y="89.01400" w="38.88900" h="7.47900" t="3:55.000"/>
