    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="20.43700" t="0:26.331"/>
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="20.43700" t="0:34.100"/>
