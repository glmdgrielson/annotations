<annotation id="annotation_1559377577" type="text" style="popup" log_data="xble=1&amp;ei=FaUeXNv-O8WJ8wTw-aLQDw&amp;a-v=R3bf7uYXoXo&amp;a-type=1&amp;a-id=annotation_1559377577" itct="CAEQs-UBGP___________wEiEwib3YrUqbTfAhXFxJwKHfC8CPoo-B0yAml2SPrC3rDu_be7Rw">
