    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="10.85900" t="4:06.400"/>
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="10.85900" t="4:13.000"/>
