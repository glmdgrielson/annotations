<annotation id="annotation_2202837061" type="text" style="popup" log_data="ei=gLMeXJTHNMqVhwaV8beACQ&amp;a-id=annotation_2202837061&amp;a-v=iy8beofRctc&amp;a-type=1&amp;xble=1" itct="CAUQs-UBGP___________wEiEwiUh4S0t7TfAhXKysEKHZX4DZAo-B0yAml2SNflxb6o78aXiwE">
  <TEXT>And the other unfinished stage is...</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="17.81200" y="0.00000" w="59.79600" h="7.19700" t="7:53.477"/>
      <rectRegion x="17.81200" y="0.00000" w="59.79600" h="7.19700" t="7:55.800"/>
