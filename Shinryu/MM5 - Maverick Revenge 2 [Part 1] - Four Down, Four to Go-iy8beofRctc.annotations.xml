    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="12.26800" t="2:20.982"/>
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="12.26800" t="2:26.500"/>
