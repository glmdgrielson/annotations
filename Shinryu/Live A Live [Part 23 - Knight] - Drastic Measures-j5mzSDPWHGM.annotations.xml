    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="20.27800" t="11:06.161"/>
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="20.27800" t="11:17.400"/>
    </movingRegion>
  </segment>
