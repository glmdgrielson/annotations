    <movingRegion type="rect">
      <rectRegion x="0.00000" y="85.83300" w="100.00000" h="14.18100" t="0:22.107"/>
      <rectRegion x="0.00000" y="85.83300" w="100.00000" h="14.18100" t="0:29.800"/>
