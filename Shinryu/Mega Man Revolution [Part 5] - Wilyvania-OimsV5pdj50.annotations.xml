    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="22.22200" t="0:02:10.1"/>
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="22.22200" t="0:02:19.1"/>
