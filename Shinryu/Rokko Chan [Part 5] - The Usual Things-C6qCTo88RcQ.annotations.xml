    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="16.18300" t="8:48.861"/>
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="16.18300" t="8:56.600"/>
