    <movingRegion type="rect">
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="11.98600" t="6:19.042"/>
      <rectRegion x="0.00000" y="0.00000" w="100.00000" h="11.98600" t="6:24.100"/>
