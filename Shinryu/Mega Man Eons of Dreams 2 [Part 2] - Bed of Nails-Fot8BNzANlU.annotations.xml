    <movingRegion type="rect">
      <rectRegion x="0.00000" y="78.59200" w="100.00000" h="20.71800" t="0:47.891"/>
      <rectRegion x="0.00000" y="78.59200" w="100.00000" h="20.71800" t="0:59.900"/>
