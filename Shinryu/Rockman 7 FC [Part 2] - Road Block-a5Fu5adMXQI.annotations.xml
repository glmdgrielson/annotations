<annotation id="annotation_1322990525" type="text" style="popup" log_data="xble=1&amp;ei=1KweXK26L4j68wSD3q-wCw&amp;a-v=a5Fu5adMXQI&amp;a-type=1&amp;a-id=annotation_1322990525" itct="CAEQs-UBGP___________wEiEwit9MaFsbTfAhUI_ZwKHQPvC7Yo-B0yAml2SIK6sbra3NvIaw">
