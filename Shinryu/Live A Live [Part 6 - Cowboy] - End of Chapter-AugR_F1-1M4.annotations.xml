  <segment>
    <movingRegion type="rect">
      <rectRegion x="0.00000" y="91.93100" w="33.38200" h="8.06900" t="19:36.800"/>
      <rectRegion x="0.00000" y="91.93100" w="33.38200" h="8.06900" t="19:38.800"/>
